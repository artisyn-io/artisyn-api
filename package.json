{
  "name": "artisyn-api",
  "version": "1.0.0",
  "description": "Backend API for Artisyn - a decentralized protocol connecting local artisans with users",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "migrate": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "prisma:generate": "prisma generate",
    "seed": "tsx src/database/seed.ts",
    "command": "ts-node src/commands/index.ts",
    "initialize": "prisma generate && prisma migrate dev && prisma db seed",
    "postinstall": "pnpm initialize"
  },
  "prisma": {
    "seed": "tsx src/database/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toneflix/artisyn-api.git"
  },
  "keywords": [
    "artisans",
    "starknet",
    "decentralized",
    "marketplace"
  ],
  "author": "Toneflix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toneflix/artisyn-api/issues"
  },
  "homepage": "https://github.com/toneflix/artisyn-api#readme",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/method-override": "^3.0.0",
    "argon2": "^0.41.1",
    "base64url": "^3.0.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pino-http": "^10.4.0",
    "sharp": "^0.34.5",
    "simple-body-validator": "^1.3.9",
    "ua-parser-js": "^2.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "latest",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1",
    "@types/node": "^22.19.7",
    "@types/nodemailer": "^6",
    "@types/passport": "^0",
    "@types/passport-facebook": "^3",
    "@types/passport-google-oauth20": "^2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "commander": "^13.1.0",
    "eslint": "^9.25.1",
    "pg": "^8.17.2",
    "prisma": "^7.2.0",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  }
}